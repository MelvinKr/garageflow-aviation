{
  "name": "garageflow-aviation",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --filter web dev",
    "build": "pnpm --filter web build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "db:studio": "drizzle-kit studio",
    "db:introspect": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 drizzle-kit introspect",
    "db:push": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 drizzle-kit push",
    "db:seed": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 tsx infra/seed.ts",
    "db:test": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node scripts/test-pg.js"
  },
  "devDependencies": {
    "@types/pg": "^8.15.5",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.31.0",
    "tsx": "^4.19.2",
    "turbo": "^2.5.6"
  },
  "packageManager": "pnpm@9.12.0",
  "dependencies": {
    "@supabase/ssr": "0.5.0",
    "openai": "4.56.0",
    "pdfkit": "^0.17.2",
    "postgres": "3.4.5"
  }
}
