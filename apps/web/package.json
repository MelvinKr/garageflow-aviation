    "@zxing/browser": "^0.1.5",
    "dexie": "^4.0.8",
    "drizzle-orm": "^0.31.0",
    "idb-keyval": "^6.2.2",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.79",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.31.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
