    "db:studio": "drizzle-kit studio --config=drizzle.config.ts",
    "db:seed": "ts-node -r tsconfig-paths/register src/scripts/seed.ts",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@garageflow/ai": "*",
    "@garageflow/db": "*",
    "@garageflow/ui": "*",
